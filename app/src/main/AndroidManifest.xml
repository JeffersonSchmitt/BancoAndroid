<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comjeffersonschmitt.github.bancoandroid">

    <application
        android:name=".BancoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="firstapp.db" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="AA_MODELS"
            android:value="comjeffersonschmitt.github.bancoandroid.domain.User, comjeffersonschmitt.github.bancoandroid.domain.Conta" />

        <activity android:name=".MainActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity_" />
        <activity android:name=".Saque_" />
        <activity android:name=".Deposito_"></activity>
    </application>

</manifest>
